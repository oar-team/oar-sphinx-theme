@import url("css/theme.css");


body,
.wy-body-for-nav,
.wy-side-nav-search {
    background: #fcfcfc;
}

body,
.wy-body-for-nav,
.wy-nav-content-wrap,
.wy-side-nav-search {
    background: #fcfcfc;
}

.wy-menu-vertical li.current a,
.wy-side-nav-search,
.wy-nav-side {
    border-right: none;
    box-shadow: -12px 0px 6px -12px #D2D2D2 inset;
}

.wy-side-nav-search img {
    width: 158px;
    height: 106px;
    border-radius: 0;
    margin: 1em;
    padding: 0;
}

.wy-side-nav-search input[type="text"] {
    border-color: rgb(201, 201, 201);
}

div.section {
    font-family: georgia, serif;
}

h1,h2,h3,h4,h5,h6 {
    font-family: helvetica, arial, 'freesans clean', sans-serif;
}

code {
    color: #73261e;
    font-size: 80%;
}

code.descname, code.descclassname {
    border: none;
    color: #000;
    font-size: 100%;
    padding: 1px;
}

.rst-content dl:not(.docutils) {
    border-top: solid 1px #f8f8f8;
}

.rst-content dl:not(.docutils) dt {
    background: #fdfdfd;
    border-top: none;
    color: rgb(64, 64, 64);
    display: block;
    margin: 1px 0;
}
